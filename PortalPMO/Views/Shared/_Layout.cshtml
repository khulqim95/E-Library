<script type="text/javascript">
    $(document).ready(function () {

        //Untuk Ajax Loading
        $(document).ajaxStart(function () {
            $('#ajaxLoading').show();
        }).ajaxStop(function () {
            $('#ajaxLoading').hide();
        }).ajaxError(function (xhr, props) {
            $('#ajaxLoading').hide();
            if (props.status === 401) {
                location.reload();
            }
            else if (props.status === 400) {
                window.location.href = "../Error/HttpStatusErrorLayout?statusCode=400"
            }
            else if (props.status === 404) {
                window.location.href ="../Error/HttpStatusErrorLayout?statusCode=404"
            }
            else if (props.status === 500) {
                window.location.href = "../Error/HttpStatusErrorLayout?statusCode=500"
                //window.location.href = "../Account/Login"
            }
            else {
                //alert("error code-->" + props.status);
            }
            });

        getAllRolePegawai();
        $('[data-toggle="tooltip"]').tooltip();
    });

    function ShowParameterSearch(elementIdBtnShow, elementIdBtnHide, elementIdCard) {
        $("#" + elementIdBtnShow).hide();
        $("#" + elementIdBtnHide).show();
        $("#" + elementIdCard).show();

    }

    function HideParameterSearch(elementIdBtnShow, elementIdBtnHide, elementIdCard) {
        $("#" + elementIdBtnShow).show();
        $("#" + elementIdBtnHide).hide();
        $("#" + elementIdCard).hide();

    }

    function HideModal(classModal) {
        $("." + classModal).removeClass("in");
        $("." + classModal).removeClass("show");

        $(".modal-backdrop").remove();
        //$("." + classModal).hide();
    }

    function ShowModal(classModal) {
        $(".modal-backdrop").add();
        //$("." + classModal).show();
        $("." + classModal).addClass("show");

    }

  //Function untuk mendapatkan semua role pegawai
    function getAllRolePegawai() {
        var SessPegawai_Id = $('#Sess_Pegawai_Id').val();
        var SessUserRole_Id = $('#Sess_User_Role_Id').val();
        //alert("Sessionm " + SessUserRole_Id);
        SearchData = { "Pegawai_Id": SessPegawai_Id }
        UrlPartialView = "../Utility/GetAllRolePegawai";
        LoadPartialViewData(UrlPartialView, function (data) {
            $('#DropdownRolePegawai').empty();
            $.each(data, function (index, value) {
                if (value.Value == SessUserRole_Id) {
                    $('#DropdownRolePegawai').append('<option value="' + value.Value + '" selected>' + value.Name + '</option>');
                }
                else {
                    $('#DropdownRolePegawai').append('<option value="' + value.Value + '">' + value.Name + '</option>');
                }
            });
        }, SearchData);
    }

    $("#DropdownRolePegawai").change(function () {
        if (confirm("Apa anda yakin ingin mengubah kewenangan Anda? Anda akan dialihkan setelah proses berhasil")) {
            $.ajax({
                type: "POST",
                url: "../Home/RoleChanged" + "/" + $("#DropdownRolePegawai").val(),
                success: function (msg) {
                    if (msg == "Home") {
                        window.location.href = "../Home/Index";
                    }
                    else if (msg == "EmptyAccess") {
                        window.location.href = "../Error/EmptyAccess";
                    }
                    else {
                        alert("Silahkan login kembali !");
                        window.location.href = "../Login/Logout"
                    }
                    //console.log(msg.result);
                    //getMenuByRole();
                }
            });
        }
    });

    function TampilkanModal(elementId, elementIdHide) {
        if (elementIdHide != null) {
            $('#' + elementIdHide).modal('hide');
        }

        setTimeout(function () { $('#' + elementId).show(); }, 400);
       
    }


    //Untuk button back draft
    function BackDraft() {
        $('#DivContentBody').show();
        document.getElementById("DivFormBody").innerHTML = "";
    }
</script>
@RenderSection("scripts", false)
