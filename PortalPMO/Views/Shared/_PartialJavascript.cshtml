    function GetTwoFirstCharFromWord(value) {
        var abbr = "";
        value = value.split(' ');
        if (value.length < 2) {
            return value[0].substr(0, 1).toUpperCase();
        }
        else {
            for (i = 0; i < 2; i++) {
                abbr += value[i].substr(0, 1);
            }
        }
        return abbr;
    }


