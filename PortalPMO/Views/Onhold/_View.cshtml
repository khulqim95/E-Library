                                    @Html.TextBoxFor(m => m.Kecamatan, new { @readonly = true, @class = "form-control", @value = "", @autocomplete = "off" })
                                </div>
                            </div>
                        </div>
                        <div class="form-group row m-b-15">
                            <label class="col-md-3 col-form-label">Kelurahan</label>
                            <div class="col-md-9">
                                <div class="input-group">
                                    @Html.TextBoxFor(m => m.Kelurahan, new { @readonly = true, @class = "form-control", @value = "", @autocomplete = "off" })
                                </div>
                            </div>
                        </div>
                        <div class="form-group row m-b-15">
                            <label class="col-md-3 col-form-label">Kode Pos</label>
                            <div class="col-md-9">
                                <div class="input-group">
                                    @Html.TextBoxFor(m => m.KodePos, new { @readonly = true, @class = "form-control", @value = "", @autocomplete = "off" })
                                </div>
                            </div>
                        </div>
                        <div class="form-group row m-b-15">
                            <label class="col-md-3 col-form-label">No Faksimilli</label>
                            <div class="col-md-9">
                                <div class="input-group">
                                    @Html.TextBoxFor(m => m.NoFax, new { @readonly = true, @class = "form-control", @value = "", @autocomplete = "off" })
                                </div>
                            </div>
                        </div>
                        <div class="form-group row m-b-15">
                            <label class="col-md-3 col-form-label">Website</label>
                            <div class="col-md-9">
                                <div class="input-group">
                                    @Html.TextBoxFor(m => m.Website, new { @readonly = true, @class = "form-control", @value = "", @autocomplete = "off" })
                                </div>
                            </div>
                        </div>
                        <div class="form-group row m-b-15">
                            <label class="col-md-3 col-form-label">Nama Unit</label>
                            <div class="col-md-9">
                                <div class="input-group">
                                    @Html.TextBoxFor(m => m.NamaUnit, new { @readonly = true, @class = "form-control", @value = "", @autocomplete = "off" })
                                </div>
                            </div>
                        </div>
                        <div class="form-group row m-b-15">
                            <label class="col-md-3 col-form-label">Sektor Ekonomi 10</label>
                            <div class="col-md-9">
                                <div class="input-group">
                                    @Html.TextBoxFor(m => m.SektorEkonomi10_Name, new { @readonly = true, @class = "form-control", @value = "", @autocomplete = "off" })
                                </div>
                            </div>
                        </div>
                        <div class="form-group row m-b-15">
                            <label class="col-md-3 col-form-label">Sektor Ekonomi 20</label>
                            <div class="col-md-9">
                                <div class="input-group">
                                    @Html.TextBoxFor(m => m.SektorEkonomi20_Name, new { @readonly = true, @class = "form-control", @value = "", @autocomplete = "off" })
                                </div>
                            </div>
                        </div>
                        <div class="form-group row m-b-15">
                            <label class="col-md-3 col-form-label">Sub Sektor Ekonomi</label>
                            <div class="col-md-9">
                                <div class="input-group">
                                    @Html.TextBoxFor(m => m.SubSektorEkonomi_Name, new { @readonly = true, @class = "form-control", @value = "", @autocomplete = "off" })
                                </div>
                            </div>
                        </div>
                        <div class="form-group row m-b-15">
                            <label class="col-md-3 col-form-label">Sektor Unggulan/ Prioritas</label>
                            <div class="col-md-9">
                                <div class="input-group">
                                    @Html.TextBoxFor(m => m.SektorEkonomi20_Name, new { @readonly = true, @class = "form-control", @value = "", @autocomplete = "off" })
                                </div>
                            </div>
                        </div>
                        <div class="form-group row m-b-15">
                            <label class="col-md-3 col-form-label">Sektor Industri</label>
                            <div class="col-md-9">
                                <div class="input-group">
                                    @Html.TextBoxFor(m => m.SektorIndustri, new { @readonly = true, @class = "form-control", @value = "", @autocomplete = "off" })
                                </div>
                            </div>
                        </div>
                        <div class="form-group row m-b-15">
                            <label class="col-md-3 col-form-label">Bidang/ Jenis Usaha</label>
                            <div class="col-md-9">
                                <div class="input-group">
                                    @Html.TextBoxFor(m => m.BidangJenisUsaha, new { @readonly = true, @class = "form-control", @value = "", @autocomplete = "off" })
                                </div>
                            </div>
                        </div>
                        <div class="form-group row m-b-15">
                            <label class="col-md-3 col-form-label">Keterangan</label>
                            <div class="col-md-9">
                                <div class="input-group">
                                    @Html.TextAreaFor(m => m.Keterangan, new { @readonly = true, @class = "form-control", @value = "", @autocomplete = "off" })
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
                <div class="main-card mb-3 card">
                    <div class="headerSearch">
                        Info Fasilitas Kredit
                    </div>
                    <div class="card-body">
                        <table id="TableFasilitasKredit" class="display nowrap table table-bordered table-hover" style="width:100%">
                            <thead>
                                <tr>
                                    <th style="width: 1px">No</th>
                                    <th style="width: 1px">Aksi</th>
                                    <th>
                                        Jenis Fasilitas
                                    </th>
                                    <th>
                                        Valuta
                                    </th>
                                    <th>
                                        Maksimum Kredit
                                    </th>
                                    <th>
                                        Tujuan
                                    </th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </div>
                </div>
                <div class="main-card mb-3 card">
                    <div class="headerSearch">
                        Info Checking Document
                    </div>
                    <div class="card-body">
                        <table id="TableCheckingDocument" class="display nowrap table table-bordered table-hover" style="width:100%">
                            <thead>
                                <tr>
                                    <th style="width: 1px">No</th>
                                    <th>
                                        Dukcapil
                                    </th>
                                    <th>
                                        NPWP
                                    </th>
                                    <th>
                                        SLIK
                                    </th>
                                    <th>
                                        DHN
                                    </th>
                                    <th>
                                        OSS
                                    </th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </div>
                </div>
                <div class="main-card mb-3 card">
                    <div class="headerSearch">
                        Info Disclaimer
                    </div>
                    <div class="card-body">
                        <table id="TableDisclaimer" class="display nowrap table table-bordered table-hover" style="width:100%">
                            <thead>
                                <tr>
                                    <th style="width: 1px">No</th>
                                    <th style="width: 1px">Aksi</th>
                                    <th>
                                        Deskripsi
                                    </th>
                                    <th>
                                        Soal
                                    </th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </div>
                </div>
                <div class="main-card mb-3 card">
                    <div class="headerSearch">
                        Catatan
                    </div>
                    <div class="card-body">
                        <div class="panel-body" style="margin-right:25px !important;">
                            <form id="FormData">
                                <div>@Html.HiddenFor(m => m.Id)</div>
                                @*<textarea id="catatan" class="form-control"></textarea>*@
                                @Html.TextAreaFor(m => m.catatan, new { @class = "form-control", @value = "", @autocomplete = "off" })
                            </form>
                        </div>
                    </div>
                </div>
                <div class="main-card mb-3 card">
                    <div class="headerSearch">
                        Info Riwayat Komentar
                    </div>
                    <div class="card-body">
                        <table id="TableRiwayatKomentar" class="display nowrap table table-bordered table-hover" style="width:100%">
                            <thead>
                                <tr>
                                    <th style="width: 1px">No</th>
                                    <th>
                                        Status
                                    </th>
                                    <th>
                                        Komentar
                                    </th>
                                    <th>
                                        Role
                                    </th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </div>
                </div>
                <div class="main-card mb-3 card">
                    <div class="headerSearch">
                        Tracking
                    </div>
                    <div class="card-body">
                        <table id="TableTracking" class="display nowrap table table-bordered table-hover" style="width:100%">
                            <thead>
                                <tr>
                                    <th>
                                        Status
                                    </th>
                                    <th>
                                        Tanggal Mulai
                                    </th>
                                    <th>
                                        Tanggal Selesai
                                    </th>
                                    <th>
                                        Oleh
                                    </th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </div>
                </div>
                <a href="#" class="mt-2 btn btn-danger pull-right" style="margin-left:10px;" id="BtnReject">Reject</a>
                <a href="#" class="mt-2 btn btn-success pull-right" id="BtnApprove">Approve</a>
                @*</form>*@
            </div>
            <div class="modal-footer">
                <a href="javascript:;" class="btn btn-sm btn-white" onclick="BackDraft()">Tutup</a>
            </div>
        </div>
    </div>
</div>
